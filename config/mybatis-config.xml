<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias alias="menu" type="com.et.edusoho.admin.menu.Menu"/>
		<typeAlias alias="user" type="com.et.edusoho.admin.user.bean.User"/>
		<typeAlias alias="category" type="com.et.edusoho.admin.curriculum.bean.Category"/>
		<typeAlias alias="loginLog" type="com.et.edusoho.admin.user.bean.LoginLog"/>
		<typeAlias alias="site" type="com.et.edusoho.admin.system.bean.Site"/>
		<typeAlias alias="course" type="com.et.edusoho.course.bean.Course"/>
		<typeAlias alias="lesson" type="com.et.edusoho.course.bean.Lesson"/>
		<typeAlias alias="chapter" type="com.et.edusoho.course.bean.Chapter"/>
		<typeAlias alias="courseFile" type="com.et.edusoho.course.bean.CourseFile"/>
		<typeAlias alias="estimate" type="com.et.edusoho.course.bean.Estimate"/>
		<typeAlias alias="number" type="com.et.edusoho.course.bean.Number" />
	</typeAliases>
	
	<mappers>
		<mapper resource="context.xml"/>
		<mapper resource="login.xml"/>
		<mapper resource="category.xml"/>
		<mapper resource="user.xml"/>
		<mapper resource="login_log.xml"/>
		<mapper resource="site.xml"/>
		<mapper resource="course.xml"/>
		<mapper resource="lesson.xml"/>
		<mapper resource="chapter.xml"/>
		<mapper resource="course_file.xml"/>
		<mapper resource="estimate.xml"/>
		<mapper resource="number.xml"/>
	</mappers>
</configuration>